<div class="padre">
    <div class="container_superior">
        <img src="https://scontent.fbog14-1.fna.fbcdn.net/v/t31.0-8/27907878_1620300761395675_7047758783445828495_o.jpg?_nc_cat=110&_nc_sid=09cbfe&_nc_ohc=nMlageIiD4gAX-LO6g6&_nc_ht=scontent.fbog14-1.fna&oh=7153ee762a08d3aa512fc653c82491d0&oe=5F75B85D"
            class="rounded-circle" alt="Cinque Terre">

        <!-- Content here -->


    </div>

    <div class="container_central">

        <h4>{{user.profesion}}</h4>
        <h2>{{user.nombre}} {{user.apellido}}</h2>
        <h6>{{user.perfil}}</h6>
        <div class="mt-4"></div>

        <div class="modal-body row">

            <div class="col-md-6">

                <div class="container_proyectos">
                    <p><b>Proyectos realizados</b></p>
                    {{#each proyectos}}
                    <div class="card cardpadre" style="width: 18rem;">
                        <img class="card-img-top"
                            src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcSQPhkyrNifsnWrHo8mlU5Yr_KjwMjloU_QFw&usqp=CAU"
                            alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title"><b>{{nombre}}</b></h5>
                            <p class="card-text">{{descripcion}}</p>
                            <a href="#" class="btn btn-primary">Boton proyecto</a>
                        </div>
                    </div>
                    <div class="mt-3"></div>
                    {{/each}}
                </div>

            </div>

            <div class="col-md-6">

                <div class="container_trabajos">
                    <p><b>Trabajos desempe√±ados</b></p>
                    {{#each trabajos}}
                    <div class="card cardpadre" style="width: 18rem;">
                        <div class="card-body">
                            <p class="card-title textcardtrabajos"><b>Empresa:</b> {{empresa}}</p>
                            <p class="card-subtitle mb-2 text-muted textcardtrabajos">{{cargo}}</p>
                            <p class="card-text textcardtrabajos">{{descripcion}}</p>
                            <a href="#" class="card-link">Boton 1</a>
                            <a href="#" class="card-link">Boton 2</a>
                        </div>
                    </div>
                    <div class="mt-3"></div>
                    {{/each}}
                </div>
            </div>

        </div>

    </div>




    {{> footer}}
</div>